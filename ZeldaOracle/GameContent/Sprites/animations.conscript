# FOUNTAINS

ANIM tile_fountain_path_nw; SUBSTRIP repeat;
	ADD frame, 1, (10, 14); ADD emptyframe, 1;
	REPEAT 2, 2;
	ADD frame, 1, (10, 15); ADD emptyframe, 1;
	REPEAT 2, 2;
	REPEAT 12, 3;
	INSERT strip, 0, 12, 4, (12, 14), (0, 0), -1; END;
ANIM tile_fountain_path_ne; SUBSTRIP repeat;
	ADD frame, 1, (11, 14); ADD emptyframe, 1;
	REPEAT 2, 2;
	ADD frame, 1, (11, 15); ADD emptyframe, 1;
	REPEAT 2, 2;
	REPEAT 12, 3;
	INSERT strip, 0, 12, 4, (12, 15), (0, 0), -1; END;
ANIM tile_fountain_path_sw; SUBSTRIP repeat;
	ADD strip, 12, 4, (12, 16); END;
ANIM tile_fountain_path_se; SUBSTRIP repeat;
