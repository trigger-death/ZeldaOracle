SOURCE "general_tiles";

ANIM tile_trampoline_bounce; SUBSTRIP clamp;
	ADD frame, 12, (4, 3);
	ADD frame,  2, (3, 3);
	ADD frame,  8, (5, 3);
	ADD frame,  1, (3, 3); END;
ANIM tile_roller_horizontal_roll;
	SUBSTRIP clamp; ADD frame, 9, (0, 3); ADD frame, 12, (1, 3); ADD frame, 10, (2, 3); ADD frame, 1, (0, 3);
	SUBSTRIP clamp; ADD frame, 9, (0, 3); ADD frame, 12, (2, 3); ADD frame, 10, (1, 3); ADD frame, 1, (0, 3); END;
ANIM tile_roller_vertical_roll;
	SUBSTRIP clamp; ADD frame, 9, (0, 4); ADD frame, 12, (2, 4); ADD frame, 10, (1, 4); ADD frame, 1, (0, 4);
	SUBSTRIP clamp; ADD frame, 9, (0, 4); ADD frame, 12, (1, 4); ADD frame, 10, (2, 4); ADD frame, 1, (0, 4); END;
	
ANIM tile_crossing_gate_raise;
	SUBSTRIP clamp; ## right
		ADD frame, 8, (4, 5), (9, -16);
		ADD part,  8, (4, 6), (9, 0);
		ADD part,  8, (9, 5), (16, 0);

		ADD frame, 8, (4, 5), (9, -16);
		ADD part,  8, (4, 6), (9, 0);
		ADD part,  8, (7, 5), (16, -16);
		ADD part,  8, (7, 6), (16, 0);
		
		ADD frame, 8, (3, 5), (9, -16);
		ADD part,  8, (3, 6), (9, 0);

	SUBSTRIP clamp; ## left
		ADD frame, 8, (6, 5), (0, -16);
		ADD part,  8, (6, 6), (0, 0);
		ADD part,  8, (9, 6), (-16, 0);

		ADD frame, 8, (6, 5), (0, -16);
		ADD part,  8, (6, 6), (0, 0);
		ADD part,  8, (8, 5), (-16, -16);
		ADD part,  8, (8, 6), (-16, 0);
		
		ADD frame, 8, (5, 5), (0, -16);
		ADD part,  8, (5, 6), (0, 0);
	END;
	
ANIM tile_crossing_gate_lower;
	SUBSTRIP clamp; ## right
		ADD frame, 8, (3, 5), (9, -16);
		ADD part,  8, (3, 6), (9, 0);

		ADD frame, 8, (4, 5), (9, -16);
		ADD part,  8, (4, 6), (9, 0);
		ADD part,  8, (7, 5), (16, -16);
		ADD part,  8, (7, 6), (16, 0);

		ADD frame, 8, (4, 5), (9, -16);
		ADD part,  8, (4, 6), (9, 0);
		ADD part,  8, (9, 5), (16, 0);
		
	SUBSTRIP clamp; ## left
		ADD frame, 8, (5, 5), (0, -16);
		ADD part,  8, (5, 6), (0, 0);

		ADD frame, 8, (6, 5), (0, -16);
		ADD part,  8, (6, 6), (0, 0);
		ADD part,  8, (8, 5), (-16, -16);
		ADD part,  8, (8, 6), (-16, 0);

		ADD frame, 8, (6, 5), (0, -16);
		ADD part,  8, (6, 6), (0, 0);
		ADD part,  8, (9, 6), (-16, 0);
	END;
	
	
ANIM minecart;
	SUBSTRIP repeat; # horizontal
		ADD frame, 6, (1, 5), (0, -3); ADD part, 6, (2, 5), (0, -2); 
		ADD frame, 6, (0, 5), (0, -3); ADD part, 6, (2, 5), (0, -3); 
	SUBSTRIP repeat; # vertical
		ADD frame, 6, (1, 6), (0, -3); ADD part, 6 (2, 6), (-1, -3);
		ADD frame, 6, (0, 6), (0, -3); ADD part, 6 (2, 6), (0, -3);
	END;


ANIM turnstile_arrows_clockwise; SUBSTRIP repeat;
	ADD frame, 12, (3, 7), (16,  0);
	ADD frame, 12, (5, 7), (32, 16);
	ADD frame, 12, (5, 9), (16, 32);
	ADD frame, 12, (3, 9), ( 0, 16);
	END;
ANIM turnstile_arrows_counterclockwise; SUBSTRIP repeat;
	ADD frame, 12, (3, 10), (16,  0);
	ADD frame, 12, (3, 12), ( 0, 16);
	ADD frame, 12, (5, 12), (16, 32);
	ADD frame, 12, (5, 10), (32, 16);
	END;
ANIM turnstile_rotate_clockwise;
	SUBSTRIP clamp;
		ADD frame, 4, turnstile_bars_angled_clockwise;
		ADD frame, 4, turnstile_bars_straight;
		ADD frame, 4, turnstile_bars_angled_counterclockwise;
		ADD frame, 4, turnstile_bars_angled;
	SUBSTRIP clamp;
		ADD frame, 4, turnstile_bars_angled_clockwise_blue;
		ADD frame, 4, turnstile_bars_straight_blue;
		ADD frame, 4, turnstile_bars_angled_counterclockwise_blue;
		ADD frame, 4, turnstile_bars_angled_blue;
	END;
ANIM turnstile_rotate_counterclockwise;
	SUBSTRIP clamp;
		ADD frame, 4, turnstile_bars_angled_counterclockwise;
		ADD frame, 4, turnstile_bars_straight;
		ADD frame, 4, turnstile_bars_angled_clockwise;
		ADD frame, 4, turnstile_bars_angled;
	SUBSTRIP clamp;
		ADD frame, 4, turnstile_bars_angled_counterclockwise_blue;
		ADD frame, 4, turnstile_bars_straight_blue;
		ADD frame, 4, turnstile_bars_angled_clockwise_blue;
		ADD frame, 4, turnstile_bars_angled_blue;
	END;
