ANIM tile_door_closed;
	SUBSTRIP repeat; ADD frame, 1, tile_door_right;
	SUBSTRIP repeat; ADD frame, 1, tile_door_up;
	SUBSTRIP repeat; ADD frame, 1, tile_door_left;
	SUBSTRIP repeat; ADD frame, 1, tile_door_down;
	END;

ANIM tile_door_close;
	SUBSTRIP clamp;	ADD frame, 6, tile_door_right, (-8, 0);
					ADD frame, 6, tile_door_right;
	SUBSTRIP clamp;	ADD frame, 6, tile_door_up, (0, 8);
					ADD frame, 6, tile_door_up;
	SUBSTRIP clamp;	ADD frame, 6, tile_door_left, (8, 0);
					ADD frame, 6, tile_door_left;
	SUBSTRIP clamp;	ADD frame, 6, tile_door_down, (0, -8);
					ADD frame, 6, tile_door_down;
	END;
	
ANIM tile_door_open;
	SUBSTRIP clamp;	ADD frame, 6, tile_door_right, (-8, 0);
					ADD emptyframe, 6;
	SUBSTRIP clamp;	ADD frame, 6, tile_door_up, (0, 8);
					ADD emptyframe, 6;
	SUBSTRIP clamp;	ADD frame, 6, tile_door_left, (8, 0);
					ADD emptyframe, 6;
	SUBSTRIP clamp;	ADD frame, 6, tile_door_down, (0, -8);
					ADD emptyframe, 6;
	END;

